    "url": "https://github.com/maxim7478/svg_sprite_plugin_vite.git"
  },
  "bugs": {
    "url": "https://github.com/maxim7478/svg_sprite_plugin_vite/issues"
  },
  "homepage": "https://github.com/maxim7478/svg_sprite_plugin_vite#readme",
